{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "JobSettings": {
    "StartHour": 16, // 9:30 PM IST is 4:00 PM UTC
    "StartMinute": 0, // 30 minutes past the hour
    "IntervalInHours": 1 // Run every 1 hours
  },
  "AllowedHosts": "*"
}
